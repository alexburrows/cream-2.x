{
	"locale": "so",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality, %administrativeArea %postalCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"administrativeArea"
	],
	"uppercase_fields": [
		"addressLine1",
		"addressLine2",
		"locality",
		"administrativeArea"
	],
	"administrative_area_type": "province",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "[A-Z]{2}[ ]?\\d{5}"
}