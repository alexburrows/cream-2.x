{
	"locale": "ca",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%postalCode %locality",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality"
	],
	"uppercase_fields": [
		"locality"
	],
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "AD[1-7]0\\d"
}