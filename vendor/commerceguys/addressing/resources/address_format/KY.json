{
	"locale": "en",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%administrativeArea %postalCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"administrativeArea"
	],
	"uppercase_fields": [
		"locality"
	],
	"administrative_area_type": "island",
	"postal_code_type": "postal",
	"postal_code_pattern": "KY\\d-\\d{4}"
}