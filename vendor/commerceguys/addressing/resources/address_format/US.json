{
	"locale": "en",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality, %administrativeArea %postalCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"administrativeArea",
		"postalCode"
	],
	"uppercase_fields": [
		"locality",
		"administrativeArea"
	],
	"administrative_area_type": "state",
	"locality_type": "city",
	"postal_code_type": "zip",
	"postal_code_pattern": "(\\d{5})(?:[ \\-](\\d{4}))?"
}