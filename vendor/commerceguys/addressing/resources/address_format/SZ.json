{
	"locale": "und",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality\n%postalCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality"
	],
	"uppercase_fields": [
		"addressLine1",
		"addressLine2",
		"locality",
		"postalCode"
	],
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "[HLMS]\\d{3}"
}