{
	"locale": "und",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%postalCode %locality %administrativeArea",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"administrativeArea",
		"postalCode"
	],
	"uppercase_fields": [
		"locality",
		"administrativeArea"
	],
	"administrative_area_type": "island",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "987\\d{2}"
}