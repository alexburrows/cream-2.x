{
	"locale": "en",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality %postalCode\n%administrativeArea",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality"
	],
	"uppercase_fields": [
		"locality",
		"administrativeArea"
	],
	"administrative_area_type": "state",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "(?:\\d{6})?"
}