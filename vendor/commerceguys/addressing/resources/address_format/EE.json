{
	"locale": "und",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%postalCode %locality %administrativeArea",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"postalCode"
	],
	"uppercase_fields": [
		"locality"
	],
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "\\d{5}",
	"administrative_area_type": "county"
}