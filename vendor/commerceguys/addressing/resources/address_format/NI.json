{
	"locale": "es",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%postalCode\n%locality, %administrativeArea",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality"
	],
	"uppercase_fields": [
		"locality",
		"administrativeArea"
	],
	"administrative_area_type": "department",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "\\d{5}"
}