{
	"locale": "es",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality %postalCode, %administrativeArea",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"administrativeArea"
	],
	"uppercase_fields": [
		"locality",
		"administrativeArea"
	],
	"administrative_area_type": "province",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "\\d{4}"
}