{
	"locale": "en",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality\n%administrativeArea %postalCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality"
	],
	"uppercase_fields": [
		"locality"
	],
	"administrative_area_type": "county",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "[\\dA-Z]{3}[ ]?[\\dA-Z]{4}"
}