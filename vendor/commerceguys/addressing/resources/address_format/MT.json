{
	"locale": "und",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality %postalCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality"
	],
	"uppercase_fields": [
		"locality",
		"postalCode"
	],
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "[A-Z]{3}[ ]?\\d{2,4}"
}