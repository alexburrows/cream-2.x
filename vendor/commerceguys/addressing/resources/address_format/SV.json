{
	"locale": "es",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%postalCode-%locality\n%administrativeArea",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"administrativeArea"
	],
	"uppercase_fields": [
		"locality",
		"administrativeArea",
		"postalCode"
	],
	"administrative_area_type": "province",
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "CP [1-3][1-7][0-2]\\d"
}