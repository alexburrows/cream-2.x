{
	"locale": "und",
	"format": "%organization\n%recipient\n%addressLine1\n%addressLine2\n%postalCode %locality %sortingCode",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"postalCode"
	],
	"uppercase_fields": [
		"locality",
		"sortingCode"
	],
	"locality_type": "city",
	"postal_code_type": "postal",
	"postal_code_pattern": "\\d{2}[ ]?\\d{3}"
}